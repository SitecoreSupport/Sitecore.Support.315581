<?xml version="1.0" encoding="utf-8"?>

<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:role="http://www.sitecore.net/xmlconfig/role/" xmlns:security="http://www.sitecore.net/xmlconfig/security/">
  <sitecore role:require="Standalone or ContentDelivery or ContentManagement">
    <pipelines>
      <owin.cookieAuthentication.signedIn>
        <processor type="Sitecore.Support.Owin.Authentication.Pipelines.CookieAuthentication.SignedIn.UpdateStatistics, Sitecore.Support.315581" resolve="true"
                   patch:before="processor[@type='Sitecore.Owin.Authentication.Pipelines.CookieAuthentication.SignedIn.ApplyRedirect, Sitecore.Owin.Authentication']"   />
      </owin.cookieAuthentication.signedIn>
    </pipelines>
  </sitecore>
</configuration>